<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroud"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/btnfish"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fish" />

    <ImageView
        android:id="@+id/btnbug"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bug" />

    <ImageView
        android:id="@+id/btnfossil"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnfish"
        app:srcCompat="@drawable/fossil" />

    <ImageView
        android:id="@+id/btnarts"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="30dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnbug"
        app:srcCompat="@drawable/acts" />

    <ImageView
        android:id="@+id/btnpeople"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnfossil"
        app:srcCompat="@drawable/people" />

    <ImageView
        android:id="@+id/btnbook"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="30dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnarts"
        app:srcCompat="@drawable/book" />

    <TextView
        android:id="@+id/fishtext"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:gravity="center_horizontal"
        android:text="水产"
        android:textColor="#000"
        android:textSize="10pt"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btnfossil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnfish" />

    <TextView
        android:id="@+id/bugtext"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="30dp"
        android:text="昆虫"
        app:layout_constraintBottom_toTopOf="@+id/btnarts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnbug"
        android:gravity="center_horizontal"
        android:textColor="#000"
        android:textSize="10pt"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/fossiltext"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:text="化石"
        app:layout_constraintBottom_toTopOf="@+id/btnpeople"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnfossil"
        android:gravity="center_horizontal"
        android:textColor="#000"
        android:textSize="10pt"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/artstext"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="30dp"
        android:text="艺术品"
        app:layout_constraintBottom_toTopOf="@+id/btnbook"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnarts"
        android:gravity="center_horizontal"
        android:textColor="#000"
        android:textSize="10pt"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/peopletext"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:text="岛民"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnpeople"
        android:gravity="center_horizontal"
        android:textColor="#000"
        android:textSize="10pt"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="30dp"
        android:text="攻略"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnbook"
        android:gravity="center_horizontal"
        android:textColor="#000"
        android:textSize="10pt"
        android:textStyle="bold"/>

</androidx.constraintlayout.widget.ConstraintLayout>